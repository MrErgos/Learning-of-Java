import java.util.HashSet;                                                                                                                                                                                                        
import java.util.Set;                                                                                                                                                                                                        
                                                                                                                                                                                                                                                                                                                                                                                                               
public class Solution {                                                                                                                                                                                                        
    private final String first, last;                                                                                                                                                                                                        
                                                                                                                                                                                                        
    public Solution(String first, String last) {                                                                                                                                                                                                        
        this.first = first;                                                                                                                                                                                                        
        this.last = last;                                                                                                                                                                                                        
    }
    /*
    This method was not
    */                                                                                                                                                                                                        
    @Override                                                                                                                                                                                                                                                                                                            
    public int hashCode() {                                                                                                                                                                                                                                                                                                              
        final int prime = 31;                                                                                                                                                                                                                                                                                                            
        int result = 1;                                                                                                                                                                                                                                                                                                            
        result = prime * result + (first == null ? 0 : first.hashCode());                                                                                                                                                                                                                                                                                                            
        result = prime * result + (last == null ? 0 : last.hashCode());                                                                                                                                                                                                                                                                                                            
        return result;                                                                                                                                                                                                                                                                                                                                                              
    }                                                                                                                                                                                                                                                                                                                                                        
    /* //This one was like that
    public boolean equals(Object o) {                                                  
        if (!(o instanceof Solution))                                                  
            return false;                                                  
        Solution n = (Solution) o;                                                  
        return n.first.equals(first) && n.last.equals(last);                                                  
    }
    */                                                                                                                                                                                                                                                                                                        
    public boolean equals(Object n) {                                                                                                                                                                                                                                                                                                            
        if (this == n) return true;                                                                                                                                                                                                                                                                                                            
        if (n == null || getClass() != n.getClass()) return false;                                                                                                                                                                                                                                                                                                            
        if (n instanceof Solution) {                                                                                                                                                                                                                                                                                                            
            Solution s = (Solution) n;                                                                                                    
            if (first != null && !first.equals(s.first))                                                                                                     
                return false;                                                                                                        
            if (last != null && !last.equals(s.last))                                                                                                     
                return false;                                                                                                    
            if (first == null && first == s.first && last == null && last == s.last)                                                                                                     
                return true;                                                                                                        
            if (first != null && (first.equals(s.first) || first == s.first) && last == null && last == s.last)                                                                                                     
                return true;                                                                                                    
            if (first != null && (first.equals(s.first) || first == s.first) && last != null && (last.equals(s.last) || last == s.last))                                                                                                     
                return true;                                                                                                        
            if (first == null && first == s.first && last != null && (last.equals(s.last) || last == s.last))                                                                                                     
                return true;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
        }                                                                                                                                                                                                                                                                                                            
        return false;                                                                                                                                                                                                                                                                                                            
    }                                                                                                                                                                                                                
                                                                                                                                                                                                           
                                                                                                                                                                                                        
    public static void main(String[] args) {                                                                                                                                                                                                        
        Set<Solution> s = new HashSet<>();                                                                                                                                                                                                        
        s.add(new Solution("Mickey", "Mouse"));                                                                                                                                                                                                        
        System.out.println(s.contains(new Solution("Mickey", "Mouse")));                                                                                                                                                                                                        
    }                                                                                                                                                                                                        
}                       
